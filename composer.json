{
    "name": "dynamic/silverstripe-foxy",
    "description": "Foxy.io integration for SilverStripe websites.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "ecommerce",
        "foxy",
        "shop"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "bummzack/sortablefile": "^2.2",
        "unclecheese/display-logic": "^3.0",
        "silverstripe/recipe-cms": "^5.0",
        "symbiote/silverstripe-gridfieldextensions": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Foxy\\": "src/",
            "Dynamic\\Foxy\\Test\\": "tests/"
        },
        "files": [
            "thirdparty/foxycart/foxycart.cart_validation.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}